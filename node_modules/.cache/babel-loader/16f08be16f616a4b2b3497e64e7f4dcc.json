KAAK,EAAE;AAAA,iBAAM,CAAC,GAAD,EAAM,CAAN,CAAN;AAAA;AAhBF,OAAP;AAkBD;;;;EAzBiCX,I;;SAAfI,M","sourcesContent":["import { Mark } from 'tiptap'\nimport { toggleMark, markInputRule, markPasteRule } from 'tiptap-commands'\n\nexport default class Strike extends Mark {\n\n  get name() {\n    return 'strike'\n  }\n\n  get schema() {\n    return {\n      parseDOM: [\n        {\n          tag: 's',\n        },\n        {\n          tag: 'del',\n        },\n        {\n          tag: 'strike',\n        },\n        {\n          style: 'text-decoration',\n          getAttrs: value => value === 'line-through',\n        },\n      ],\n      toDOM: () => ['s', 0],\n    }\n  }\n\n  keys({ type }) {\n    return {\n      'Mod-d': toggleMark(type),\n    }\n  }\n\n  commands({ type }) {\n    return () => toggleMark(type)\n  }\n\n  inputRules({ type }) {\n    return [\n      markInputRule(/~([^~]+)~$/, type),\n    ]\n  }\n\n  pasteRules({ type }) {\n    return [\n      markPasteRule(/~([^~]+)~/g, type),\n    ]\n  }\n\n}\n"]}]}