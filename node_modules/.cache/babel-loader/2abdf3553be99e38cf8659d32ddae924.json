ap-commands'\n\nexport default class HardBreak extends Node {\n\n  get name() {\n    return 'hard_break'\n  }\n\n  get schema() {\n    return {\n      inline: true,\n      group: 'inline',\n      selectable: false,\n      parseDOM: [\n        { tag: 'br' },\n      ],\n      toDOM: () => ['br'],\n    }\n  }\n\n  keys({ type }) {\n    const command = chainCommands(exitCode, (state, dispatch) => {\n      dispatch(state.tr.replaceSelectionWith(type.create()).scrollIntoView())\n      return true\n    })\n    return {\n      'Mod-Enter': command,\n      'Shift-Enter': command,\n    }\n  }\n\n}\n"]}]}